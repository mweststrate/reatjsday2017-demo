    "Snippet1": {
        "prefix": "snippet1",
        "body": [
            "import { types } from \"mobx-state-tree\"",
            "",
            "",
            "export const Todo = types",
            "   .model({",
            "        text: \"stuff\",",
            "        completed: false,",
            "        id: types.number",
            "    })",
            "",
            "export const Store = types",
            "    .model({",
            "       filter: types.enumeration(\"filter\", [\"all\", \"active\", \"completed\"]),",
            "       todos: types.array(Todo),",
            "   })"
        ]
    },
    "Snippet2": {
        "prefix": "snippet2",
        "body": [
            "import { Store } from \"./models/store\"",
            "",
            "let store = Store.create(initialState)",
            "",
            "render(<App store={store} />, document.getElementById(\"root\"))"
        ]
    },
    "Snippet3": {
        "prefix": "snippet3",
        "body": [
            ".actions(self => ({",
            "        complete() {",
            "            self.completed = !self.completed",
            "        }",
            "}))"
        ]
    },
    "Snippet4": {
        "prefix": "snippet4",
        "body": [
            " .actions(self => ({",
            "        setFilter(filter) {",
            "            self.filter = filter",
            "        }",
            "        addTodo(text) {",
            "            self.todos.push({ text, id: Math.random() })",
            "        },",
            " }))"
        ]
    },
    "Snippet5": {
        "prefix": "snippet5",
        "body": [
            ".views(self => ({",
            "        get activeCount() {",
            "            return self.todos.filter(todo => todo.completed === true).length",
            "        },",
            "        get filteredTodos() {",
            "            switch (self.filter) {",
            "                case \"all\":",
            "                    return self.todos",
            "                case \"active\":",
            "                    return self.todos.filter(todo => !todo.completed)",
            "                case \"completed\":",
            "                    return self.todos.filter(todo => todo.completed)",
            "            }",
            "        }",
            "    }))"
        ]
    },
    "Snippet6": {
        "prefix": "snippet6",
        "body": [
            "import { onSnapshot } from \"mobx-state-tree\"",
            "",
            "const initialState = localStorage.getItem(\"store\")",
            "    ? JSON.parse(localStorage.getItem(\"store\"))",
            "    : // what was there",
            "",
            "onSnapshot(store, snapshot => localStorage.setItem(\"store\", JSON.stringify(snapshot)))"
        ]
    },
    "Snippet7": {
        "prefix": "snippet7",
        "body": [
            "import TimeTraveller from \"mobx-state-tree/middleware/TimeTraveller\"",
            "history: types.optional(TimeTraveller, { targetPath: \"../todos\" })"
        ]
    },
    "Snippet8": {
        "prefix": "snippet8",
        "body": [
            "setInterval(() => {",
            "    store.addTodo(\"TEST\")",
            "}, 5000)"
        ]
    },
    "Snippet9": {
        "prefix": "snippet9",
        "body": [
            "import UndoManager from \"mobx-state-tree/middleware/UndoManager\"",
            "history: types.optional(UndoManager, {})"
        ]
    },
    "Snippet10": {
        "prefix": "snippet10",
        "body": [
            "setInterval(() => {",
            "    store.history.withoutUndo(() => {",
            "        store.addTodo(\"TEST\")",
            "   })",
            "}, 5000)"
        ]
    },
    "Snippet11": {
        "prefix": "snippet11",
        "body": [
            "import { connectReduxDevtools } from \"mobx-state-tree/middleware/redux\"",
            "connectReduxDevtools(require(\"remotedev\"), store)"
        ]
    }
